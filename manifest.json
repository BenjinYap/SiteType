{
	"manifest_version":2,
	"name":"page type",
	"version":"1.0",

	"content_scripts":[
		{
			"matches":["http://*/*", "https://*/*"],
			"js":["js/vendor/jquery-2.1.4.min.js", "js/app.js"],
			"css":["css/app.css"]
		}
	],

	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},

	"options_ui":{
		"page":"awd.html"
	},

	"permissions":[
		"storage"
	]
}