{
	"manifest_version":2,
	"name":"page type",
	"version":"1.0",

	"content_scripts":[
		{
			"matches":["http://benjinyap.com/*"],
			"js":["jquery-2.1.4.min.js", "core.js"],
			"css":["app.css"]
		}
	],

	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},

	"options_ui":{
		"page":"awd.html"
	},

	"permissions":[
		"storage"
	]
}